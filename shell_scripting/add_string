#!/bin/bash

# Check if two arguments are provided
if [ "$#" -ne 2 ]; then
    echo "Usage: $0 <string1> <string2>"
    exit 1
fi

# Assign command line arguments to variables
string1=$1
string2=$2

# Concatenate the strings
result="${string1}${string2}"

# Print the result
echo "Concatenated string: $result"
