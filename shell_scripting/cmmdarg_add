#!/bin/bash

# Check if two arguments are provided
if [ "$#" -ne 2 ]; then
    echo "Usage: $0 <number1> <number2>"
    exit 1
fi

# Assign arguments to variables (remove spaces around '=')
number1=$1
number2=$2

# Perform addition
sum=$((number1 + number2))

# Display the result
echo "$number1 + $number2 = $sum"

